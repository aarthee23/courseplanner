<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All user course selections
    </h1>

    <a href="/user_course_selections/new" class="btn btn-block btn-outline-secondary">
      Add a new user course selection
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Course plan
        </th>

        <th>
          Course
        </th>

        <th>
          Professor
        </th>

        <th>
          Quarter
        </th>

        <th>
          Day
        </th>

        <th>
          Hours of preparation
        </th>

        <th>
          Rating
        </th>

        <th>
          Bid points
        </th>

        <th>
          Pre req
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @user_course_selections.each do |user_course_selection| %>
      <tr>
        <!-- Display user_course_selection.id start -->
        <td>
          <%= user_course_selection.id %>
        </td>
        <!-- Display user_course_selection.id end -->

        <!-- Display user_course_selection.course_plan_id start -->
        <td>
          <%= user_course_selection.course_plan.created_at %>
        </td>
        <!-- Display user_course_selection.course_plan_id end -->

        <!-- Display user_course_selection.course_id start -->
        <td>
          <%= user_course_selection.course_id %>
        </td>
        <!-- Display user_course_selection.course_id end -->

        <!-- Display user_course_selection.professor start -->
        <td>
          <%= user_course_selection.professor %>
        </td>
        <!-- Display user_course_selection.professor end -->

        <!-- Display user_course_selection.quarter start -->
        <td>
          <%= user_course_selection.quarter %>
        </td>
        <!-- Display user_course_selection.quarter end -->

        <!-- Display user_course_selection.day start -->
        <td>
          <%= user_course_selection.day %>
        </td>
        <!-- Display user_course_selection.day end -->

        <!-- Display user_course_selection.hours_of_preparation start -->
        <td>
          <%= user_course_selection.hours_of_preparation %>
        </td>
        <!-- Display user_course_selection.hours_of_preparation end -->

        <!-- Display user_course_selection.rating start -->
        <td>
          <%= user_course_selection.rating %>
        </td>
        <!-- Display user_course_selection.rating end -->

        <!-- Display user_course_selection.bid_points start -->
        <td>
          <%= user_course_selection.bid_points %>
        </td>
        <!-- Display user_course_selection.bid_points end -->

        <!-- Display user_course_selection.pre_req start -->
        <td>
          <%= user_course_selection.pre_req %>
        </td>
        <!-- Display user_course_selection.pre_req end -->

        <!-- Display user_course_selection.created_at start -->
        <td>
          <%= time_ago_in_words(user_course_selection.created_at) %> ago
        </td>
        <!-- Display user_course_selection.created_at end -->

        <!-- Display user_course_selection.updated_at start -->
        <td>
          <%= time_ago_in_words(user_course_selection.updated_at) %> ago
        </td>
        <!-- Display user_course_selection.updated_at end -->

        <td>
          <a href="/user_course_selections/<%= user_course_selection.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
